",[],[],["250","251"],"/Users/meerim/Desktop/Meerim/react/my-blogs — копия/src/pages/Article/Comments.jsx",[],[],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/index.js",[],[],["252","253"],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/App.jsx",[],[],["254","255"],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/pages/Main/Main.jsx",[],[],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/pages/Article/Article.jsx",["256"],[],"import React, {useEffect, useState} from 'react';\nimport { useParams } from 'react-router-dom';\n\nimport styles from \"./Article.module.css\"\nimport Comments from './Comments';\nimport {BASE_URL } from '../../constant';\n\nconst Article = () => {\n    const params = useParams();\n    const postId = parseInt(params.id);\n    const [post, setPosts] = useState({});\n\n    useEffect(() => {\n            const url = BASE_URL + '/posts/' + postId;\n\n            fetch(url)\n            .then(response => response.json())\n            .then(data => setPosts(data))\n    },[])\n\n    // const postData = posts.find((item) =>{\n    //     return item.id === postId;\n    //  console.log(postData);\n    // })\n\n  \n\n    return (\n        <div className={styles.main}>\n             <div className={styles.container}>\n                <img src={post.imgOfPublisher} alt=\"\" />\n                <p>{post.name} {post.dateOfPublication}</p> <br />\n            </div>\n            <h1>{post.title}</h1>\n            <div className={styles.rowTags}>\n                 {/* {post.tags.map((item) =>{\n                return <p>{item}</p>\n            })} */}\n            </div>\n           \n            <img src={post.imageUrl} alt=\"\" /> <br />\n            <p>{post.desc} </p> <br /> <br />\n             <Comments postId={postId}/> \n        </div>\n    );\n};\n\nexport default Article;","/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/pages/Article/Comments.jsx",["257"],[],"import React, {useEffect, useState} from 'react';\nimport styles from \"./Comments.module.css\"\nimport { BASE_URL } from '../../constant';\nimport toast from 'react-hot-toast';\nimport { getCurrentDate } from '../../helpers';\n\nconst Comments = (props) => {\n    console.log(props)\n    const [name, setName] = useState('');\n    const [comment, setComment] = useState('');\n    const [comments,setComments] =useState([]);\n\n    const getName = (event) => {\n        setName(event.currentTarget.value);\n    }\n\n    const getComment =(event) => {\n        setComment(event.currentTarget.value);\n    }\n\n    const getCommentsByPostId = () => {\n        const url = BASE_URL + \"/comments?postId=\" +props.postId;\n        console.log(url);\n\n        fetch(url)\n        .then(response => {\n            if(response.status === 200){\n                return response.json();\n            } else {\n                toast.error('Загрузка комментариев не удалась,пожалуйста поп попробуйте снова')\n            }\n        })\n        .then(data => setComments(data))\n\n    }\n\n    const addComment = () => {\n        const obj = {\n            name: name,\n            comment: comment,\n            postId:props.postId,\n            date:getCurrentDate()\n        }\n\n     \n       const url = BASE_URL + \"/comments\";\n\n       console.log(url)\n    \n       const options = {\n          method: \"POST\",\n          headers: {\n            \"Content-Type\": \"application/json\"\n          },\n          body: JSON.stringify(obj)\n        }\n    \n        fetch(url,options)\n            .then(response => {\n                if(response.status === 201){\n                    toast.success(\"Ваш комментарий добавлен\");\n                    getCommentsByPostId();\n                } else {\n                     toast.error(\"Произошла ошибка\")\n                }\n            })\n            // .then((data) => console.log(data))\n      }\n\n    useEffect(() => {\n       getCommentsByPostId();\n    },[])\n\n    return (\n        <div className={styles.comments}>\n            <h4>Комментарии</h4>\n            <div className={styles.container}>\n                <img src=\"https://sevelina.ru/images/uploads/2014/02/287.png\" alt=\"\" />\n                <input onChange={getName} className={styles.container1} type=\"text\" placeholder='Введите имя'/>\n                <input onChange={getComment}  type=\"text\" placeholder='Введите текст комментария'/>\n                <button onClick={addComment}>Оставить комментарий</button> \n            </div><br /> <br />\n            <h5>122 комментариев {\n                 comments.map((item) => {\n                     return <>\n                                <div key={item.id} className={styles.container}>\n                                    {/* <img src={item.img} alt=\"\" /> */}\n                                    <p>{item.name}</p> <br /> \n                                </div>\n                                <p>{item.comment}</p>\n                                <h6>Ответить</h6>\n                            </>\n                 })\n            }\n            </h5> \n        </div>\n    );\n};\n\nexport default Comments;",["258","259"],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/components/Header/Header.jsx",[],[],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/components/BlogEntries/BlogEntries.jsx",[],[],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/components/Sidebar/Sidebar.jsx",[],[],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/components/Footer/Footer.jsx",["260"],[],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/constant/index.js",[],[],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/components/BlogEntries/BlogItem.jsx",[],[],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/components/Tags/Tags.jsx",[],[],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/components/PopularPosts/PopularPosts.jsx",["261"],[],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/components/AboutMe/AboutMe.jsx",["262"],[],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/components/Test/Test.jsx",[],[],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/helpers/index.js",[],[],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/components/Test/Goods.jsx",["263","264","265"],[],"import React, { useEffect, useState } from \"react\";\nimport styles from \"../Test/Goods.module.css\";\nimport classNames from \"classnames\";\nimport Button from \"../Button/Button\";\nimport { BASE_URL } from \"../../constant\";\nimport toast from \"react-hot-toast\";\n\n\nconst GoodsCard = ({item}) => {\n  const [count,setCount] = useState(1);\n  const [cart, setCart] = useState({});\n\n  const addCart = () => {\n    let cartStorage = localStorage.getItem('cart');\n\n    const id = item.id;\n    let object = {\n      [id]: {\n        ...item,\n      count: 1\n    }\n    }\n\n    if(cartStorage){\n      cartStorage = JSON.parse(cartStorage);\n      object = {\n        ...object,\n        ...cartStorage\n      }\n    \n    }\n    localStorage.setItem(\"cart\", JSON.stringify(object));\n  };\n\n  const increase = () => {\n    setCount(count+1)\n }\n\n const decrease = () => {\n   if(count > 1) {\n     setCount(count-1)\n   }\n }\n\n  return (<div key={item.id} className={classNames(styles.goodsCard)}>\n      <h1>{item.name}</h1>\n      <p>{item.ing}</p>\n      <h3>\n        <span>{item.price}</span>сом\n      </h3>\n      <button onClick={increase}>+</button>\n      <button onClick={decrease} >-</button>\n      <p><strong>{count}</strong></p>\n      <Button myClass=\"mainBtn\" handleClick={() => addCart(item)}>\n        В корзину\n      </Button>\n    </div>\n  )\n  \n}\n\nconst Goods = () => {\n  const [goods, setGoods] = useState([]);\n  \n  const getGoods = () => {\n    const url = BASE_URL + \"/goods\";\n\n    fetch(url)\n      .then((response) => {\n        if (response.status === 200) {\n          return response.json();\n        } else {\n          toast.error(\"Что-то пошло не так. Обновите страницу\");\n        }\n      })\n      .then((data) => setGoods(data));\n  };\n\n  useEffect(() => {\n    getGoods();\n  }, []);\n\n  return (\n    <>\n      <a href=\"\">Корзина</a>\n      <div className={classNames(styles.container)}>\n        {goods.map(item => <GoodsCard key={item.id} item={item}/>)}\n      </div>\n    </>\n  )\n}\n\nexport default Goods;\n","/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/components/Button/Button.jsx",[],[],{"ruleId":"266","replacedBy":"267"},{"ruleId":"268","replacedBy":"269"},{"ruleId":"266","replacedBy":"270"},{"ruleId":"268","replacedBy":"271"},{"ruleId":"266","replacedBy":"272"},{"ruleId":"268","replacedBy":"273"},{"ruleId":"266","replacedBy":"274"},{"ruleId":"268","replacedBy":"275"},{"ruleId":"276","severity":1,"message":"277","line":7,"column":18,"nodeType":"278","endLine":7,"endColumn":187},{"ruleId":"279","severity":1,"message":"280","line":15,"column":25,"nodeType":"278","endLine":15,"endColumn":119},{"ruleId":"281","severity":1,"message":"282","line":8,"column":7,"nodeType":"283","messageId":"284","endLine":8,"endColumn":16},{"ruleId":"266","replacedBy":"285"},{"ruleId":"268","replacedBy":"286"},{"ruleId":"281","severity":1,"message":"282","line":8,"column":7,"nodeType":"283","messageId":"284","endLine":8,"endColumn":16},{"ruleId":"276","severity":1,"message":"277","line":7,"column":18,"nodeType":"278","endLine":7,"endColumn":187},{"ruleId":"279","severity":1,"message":"280","line":15,"column":25,"nodeType":"278","endLine":15,"endColumn":119},{"ruleId":"266","replacedBy":"287"},{"ruleId":"268","replacedBy":"288"},{"ruleId":"266","replacedBy":"289"},{"ruleId":"268","replacedBy":"290"},{"ruleId":"266","replacedBy":"291"},{"ruleId":"268","replacedBy":"292"},{"ruleId":"293","severity":1,"message":"294","line":19,"column":7,"nodeType":"295","endLine":19,"endColumn":9,"suggestions":"296"},{"ruleId":"293","severity":1,"message":"297","line":72,"column":7,"nodeType":"