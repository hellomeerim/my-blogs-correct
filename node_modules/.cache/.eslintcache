 getGoods();\n  }, []);\n\n  return (\n    <>\n      <a href=\"\">Корзина</a>\n      <div className={classNames(styles.container)}>\n        {goods.map(item => <GoodsCard key={item.id} item={item}/>)}\n      </div>\n    </>\n  )\n}\n\nexport default Goods;\n","/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/components/Button/Button.jsx",[],[],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/context/LangContext.js",[],[],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/context/ThemeContext.js",[],[],{"ruleId":"283","replacedBy":"284"},{"ruleId":"285","replacedBy":"286"},{"ruleId":"283","replacedBy":"287"},{"ruleId":"285","replacedBy":"288"},{"ruleId":"283","replacedBy":"289"},{"ruleId":"285","replacedBy":"290"},{"ruleId":"283","replacedBy":"291"},{"ruleId":"285","replacedBy":"292"},{"ruleId":"293","severity":1,"message":"294","line":7,"column":18,"nodeType":"295","endLine":7,"endColumn":187},{"ruleId":"296","severity":1,"message":"297","line":15,"column":25,"nodeType":"295","endLine":15,"endColumn":119},{"ruleId":"298","severity":1,"message":"299","line":8,"column":7,"nodeType":"300","messageId":"301","endLine":8,"endColumn":16},{"ruleId":"283","replacedBy":"302"},{"ruleId":"285","replacedBy":"303"},{"ruleId":"298","severity":1,"message":"299","line":8,"column":7,"nodeType":"300","messageId":"301","endLine":8,"endColumn":16},{"ruleId":"293","severity":1,"message":"294","line":7,"column":18,"nodeType":"295","endLine":7,"endColumn":187},{"ruleId":"296","severity":1,"message":"297","line":15,"column":25,"nodeType":"295","endLine":15,"endColumn":119},{"ruleId":"283","replacedBy":"304"},{"ruleId":"285","replacedBy":"305"},{"ruleId":"283","replacedBy":"306"},{"ruleId":"285","replacedBy":"307"},{"ruleId":"283","replacedBy":"308"},{"ruleId":"285","replacedBy":"309"},{"ruleId":"283","replacedBy":"310"},{"ruleId":"285","replacedBy":"311"},{"ruleId":"312","severity":1,"message":"313","line":19,"column":7,"nodeType":"314","endLine":19,"endColumn":9,"sugge