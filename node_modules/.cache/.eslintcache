<h1>{lang}</h1>\n                    <h4 onClick={clickTag}>{tag}</h4>\n                </div>\n                <div className=\"w3-container w3-white\">\n                <p><span className=\"w3-tag w3-black w3-margin-bottom\">Travel</span>\n                    {tags.map((item) => {\n                        return  <span key={item.id} className=\"w3-tag w3-light-grey w3-small w3-margin-bottom\" style={{marginRight:\"10px\"}}>{item.text}</span> \n                    })}\n                </p>\n              </div>\n           </div>\n        </div>\n    );\n};\n\nexport default Tags;\n","/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/components/PopularPosts/PopularPosts.jsx",["279","280"],[],"import React, {useContext} from 'react';\nimport { popularPosts } from '../../constant';\nimport LangContext from '../../context/LangContext';\n\n\nconst PopularPosts = () => {\n    const {lang, setLang} = useContext(LangContext)\n    return (\n        <>\n                <div className=\"w3-card w3-margin\">\n                    <div className=\"w3-container w3-padding\">\n                        <h1>{lang}</h1>\n                        <h4>Popular Posts</h4>\n                    </div>\n                    <ul className=\"w3-ul w3-hoverable w3-white\">\n                    {popularPosts.map((item) => {\n                    return  <li key={item.id} className=\"w3-padding-16\">\n                        <img src={item.img} alt=\"Image\" className=\"w3-left w3-margin-right\" style={{width: '50px'}} />\n                        <span className=\"w3-large\">{item.theme}</span><br />\n                        <span>{item.desc}</span>\n                     </li>\n                })}\n                    </ul>       \n                </div>\n            <hr />\n        </>\n    );\n};\n\nexport default PopularPosts;\n\n","/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/components/AboutMe/AboutMe.jsx",["281"],[],"/Users/meerim/Desktop/Meerim/react/my-blogs-correct/src/com